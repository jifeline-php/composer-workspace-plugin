{
    "name": "factorit/composer-workspace-plugin",
    "description": "Composer plugin for a monorepo workspace",
    "type": "composer-plugin",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "composer-plugin-api": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "class": "Monorepo\\Composer\\Plugin"
    },
    "config": {
        "sort-packages": true
    }
}
